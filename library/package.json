{
  "name": "yourlib",
  "type": "module",
  "version": "1.0.0",
  "description": "A Vue 3 component library",
  "author": {
    "name": "Gleb Sakharov",
    "url": "https://github.com/skhrvg"
  },
  "license": "MIT",
  "homepage": "https://github.com/skhrvg/yourlib/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/skhrvg/yourlib.git"
  },
  "bugs": {
    "url": "https://github.com/skhrvg/yourlib/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/yourlib.js"
    },
    "./style.css": "./dist/style.css"
  },
  "module": "./dist/yourlib.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "vite build --watch",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prepare": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.5.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vue-tsc": "^1.8.26"
  }
}
